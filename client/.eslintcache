kill, { error }] = useMutation(ADD_SKILL);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n\n    try {\n      const data = await addSkill({\n        variables: { profileId, skill },\n      });\n\n      setSkill('');\n    } catch (err) {\n      console.error(err);\n    }\n  };\n\n  return (\n    <div>\n      <h4>List another item for sale.</h4>\n\n      {Auth.loggedIn() ? (\n        <form\n          className=\"flex-row justify-center justify-space-between-md align-center\"\n          onSubmit={handleFormSubmit}\n        >\n          <div className=\"col-12 col-lg-9\">\n            <input\n              placeholder=\"Describe your product\"\n              value={skill}\n              className=\"form-input w-100\"\n              onChange={(event) => setSkill(event.target.value)}\n            />\n          </div>\n\n          <div className=\"col-12 col-lg-3\">\n            <button className=\"btn btn-info btn-block py-3\" type=\"submit\">\n              List Item\n            </button>\n          </div>\n          {error && (\n            <div className=\"col-12 my-3 bg-danger text-white p-3\">\n              {error.message}\n            </div>\n          )}\n        </form>\n      ) : (\n        <p>\n          You need to be logged in to list an item for sale. Please{' '}\n          <Link to=\"/login\">login</Link> or <Link to=\"/signup\">signup.</Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default SkillForm;\n",{"ruleId":"68","severity":1,"message":"69","line":37,"column":1,"nodeType":"70","endLine":37,"endColumn":34},{"ruleId":"71","severity":1,"message":"72","line":23,"column":15,"nodeType":"73","messageId":"74","endLine":23,"endColumn":19},{"ruleId":"71","severity":1,"message":"72","line":18,"column":13,"nodeType":"73","messageId":"74","endLine":18,"endColumn":17},"import/no-anonymous-default-export","Assign instance to a variable before exporting as module default","ExportDefaultDeclaration","no-unused-vars","'da